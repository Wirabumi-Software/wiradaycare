					"name": "get token admin",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Parse JSON response\r",
									"let json = pm.response.json();\r",
									"\r",
									"// Save access_token as environment variable\r",
									"if (json.access_token) {\r",
									"    pm.environment.set(\"access_token\", json.access_token);\r",
									"    console.log(\"Access token saved to environment\");\r",
									"} else {\r",
									"    console.warn(\"No access_token found in response\");\r",
									"}\r",
